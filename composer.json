{
    "name": "ubbie/vipartesanias-store",
    "description": "Vipartesanias Store",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.0 || ^2.0",
        "prestashop/autoloader": "^1.0",
        "prestashop/composer-registrar": "^1.0"
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "prestashop/composer-registrar": true,
            "prestashop/composer-installer": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\PrestaShop\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    }
}