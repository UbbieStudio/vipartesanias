{
    "name": "ubbie/vipartesanias-store",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.0 || ^2.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\PrestaShop\\Adapter\\": "src/Adapter/",
            "PrestaShop\\PrestaShop\\Core\\": "src/Core/",
            "PrestaShopBundle\\": "src/PrestaShopBundle/",
            "PrestaShop\\Autoload\\": "src/PrestaShop/Autoload/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php",
            "classes/"
        ]
    }
}